<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="model.TO">
    <class name="model.TO.info" table="info">
        <id name="id" type="integer">
            <column name="ID" sql-type="INT(6)"/>
            <generator class="increment"/>
        </id>
        <property name="health" type="java.lang.String">
            <column name="health" sql-type="VARCHAR(20)"/>
        </property>
        <property name="email" type="java.lang.String">
            <column name="email" sql-type="VARCHAR(20)"/>
        </property>
    </class>

</hibernate-mapping>
